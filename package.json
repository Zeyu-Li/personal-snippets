{
    "name": "personal-snippets",
    "displayName": "Personal Snippets",
    "description": "Tons of snippets for all different kinds of languages!",
    "version": "1.0.0",
    "publisher": "Andrew Li",
    "icon": "img/logo.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/Zeyu-Li/personal-snippets"
    },
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "html",
                "path": "./snippets/html_snippets.code-snippets"
            },
            {
                "language": "javascript",
                "path": "./snippets/js_snippets.code-snippets"
            },
            {
                "language": "typescript",
                "path": "./snippets/ts_snippets.code-snippets"
            },
            {
                "language": "lua",
                "path": "./snippets/lua_snippets.code-snippets"
            },
            {
                "language": "c",
                "path": "./snippets/c_snippets.code-snippets"
            },
            {
                "language": "cpp",
                "path": "./snippets/cpp_snippets.code-snippets"
            },
            {
                "language": "csharp",
                "path": "./snippets/cs_snippets.code-snippets"
            },
            {
                "language": "python",
                "path": "./snippets/py_snippets.code-snippets"
            },
            {
                "language": "shellscript",
                "path": "./snippets/sh_snippets.code-snippets"
            },
            {
                "language": "powershell",
                "path": "./snippets/ps_snippets.code-snippets"
            }
        ]
    }
}
